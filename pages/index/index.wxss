    display: block;
    font-size: 40rpx;
    color: #333;
    font-weight: 700;
    padding-left: 5rpx;
    border-left:10rpx solid yellow;
    padding: 10rpx;
}
#themePav .themes{
    width: 49%;
    height: 200rpx;
    display: inline-block;
    /*display: flex;
    justify-content: space-between;
    flex-wrap: wrap;*/
    margin-bottom: 10rpx;
}
#themePav .themes image{
    width: 100%;
    height: 200rpx;
    /*margin-top: 10rpx;*/
}
#choose-part{
    background-color: #eee;
}
#choose-part .choose-title{
    display: block;
    font-size: 40rpx;
    color: #333;
    font-weight: 700;
    border-left:10rpx solid yellow;
    padding: 10rpx;
}
#choose-part .chooses{
    margin-bottom: 30rpx;
}
#choose-part .chooses image{
    width: 100%;
